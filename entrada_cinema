#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main(){
    //Codigo de entrar no cinema
    
    int hora_cinema = 20;
    int hora_atual = 20;
    
    if(hora_atual > hora_cinema + 30){
        printf("Passou do tempo limite");
    } else if(hora_atual < hora_cinema -30){
        printf("Nem abriu a porta do cinema");
    } else{
        printf("Pode entrar");
    }
}